    "# Q9: Dice Simulation\n",
    "import numpy as np\n",
    "\n",
    "dice = np.random.randint(1,7,500)\n",
    "faces, counts = np.unique(dice, return_counts=True)\n",
    "print(\"Faces:\", faces)\n",
    "print(\"Counts:\", counts)\n",
    "print(\"Relative:\", counts/500)"
