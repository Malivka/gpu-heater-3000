   "source": [
    "# Q2: Sub-matrix and Determinant\n",
    "import numpy as np\n",
    "\n",
    "A = np.arange(1,17).reshape(4,4)\n",
    "sub = A[2:4, 0:2]\n",
    "\n",
    "print(\"Matrix:\\n\", A)\n",
    "print(\"Sub-matrix:\\n\", sub)\n",
    "print(\"Determinant:\", np.linalg.det(sub))"
   ]
