    "\n",
    "# Q3: Covariance and Correlation\n",
    "\n",
    "import numpy as np\n",
    "\n",
    "x = np.random.rand(100)\n",
    "y = 2*x + np.random.rand(100)\n",
    "\n",
    "cov = np.cov(x, y)\n",
    "corr = np.corrcoef(x, y)\n",
    "\n",
    "print(\"Covariance Matrix:\\n\", cov)\n",
    "print(\"Correlation Coefficient:\\n\", corr)\n"
   ]
