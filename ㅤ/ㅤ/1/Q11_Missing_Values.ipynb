    "# Q11: Missing Values\n",
    "import pandas as pd\n",
    "import numpy as np\n",
    "\n",
    "df = pd.DataFrame({\n",
    "    \"A\":[1,2,np.nan,4],\n",
    "    \"B\":[np.nan,3,4,np.nan],\n",
    "    \"C\":[1,np.nan,3,4]\n",
    "})\n",
    "\n",
    "df = df.fillna(df.mean())\n",
    "df = df.dropna(thresh=2)\n",
    "print(df)"
   ]
