    "# Q4: Coin Toss Simulation\n",
    "import numpy as np\n",
    "\n",
    "toss = np.random.randint(0,2,1000)\n",
    "heads = (toss==1).sum()\n",
    "tails = (toss==0).sum()\n",
    "\n",
    "print(\"Heads:\", heads)\n",
    "print(\"Tails:\", tails)\n",
    "print(\"Probability of Heads:\", heads/1000)"
   ]
  }
