   "source": [
    "# Q6: Transpose and Inverse\n",
    "import numpy as np\n",
    "\n",
    "A = np.arange(1,10).reshape(3,3)\n",
    "A_inv = np.linalg.inv(A)\n",
    "\n",
    "print(\"Transpose:\\n\", A.T)\n",
    "print(\"Inverse:\\n\", A_inv)\n",
    "print(\"A x A^-1:\\n\", A@A_inv)"
   ]
  }
