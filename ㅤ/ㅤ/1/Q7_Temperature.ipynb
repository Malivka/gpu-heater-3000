    "# Q7: Temperature Stats\n",
    "import numpy as np\n",
    "\n",
    "temp = np.random.randint(20,41,30)\n",
    "print(\"Hottest:\", temp.max())\n",
    "print(\"Coldest:\", temp.min())\n",
    "print(\"Mean:\", temp.mean())\n",
    "print(\"Median:\", np.median(temp))\n",
    "print(\"Std:\", temp.std())"
   ]
